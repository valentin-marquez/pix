{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "1.0.1",
  "packages": [
    "packages/library"
  ],
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": [
